<view class="content"><view class="goods-list"><block wx:for="{{cartList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['navToDetailPage',['$0'],[[['cartList','',index]]]]]]]}}" class="goods-item" bindtap="__e"><view class="image-wrapper"><image src="{{item.mainImg}}" mode="aspectFill"></image></view><text class="title clamp">{{item.stylename}}</text><view class="price-box"><block wx:if="{{pType==0||pType==1}}"><text class="price">{{item.price}}</text></block><block wx:if="{{pType==2}}"><text class="price">{{item.provincialPrice}}</text></block><block wx:if="{{pType==3}}"><text class="price">{{item.cityPrice}}</text></block><block wx:if="{{pType==4}}"><text class="price">{{item.regionPrice}}</text></block></view></view></block></view><uni-load-more vue-id="1" loadingType="{{loadingType}}" contentText="{{contentText}}" status="{{status}}" bind:__l="__l"></uni-load-more></view>