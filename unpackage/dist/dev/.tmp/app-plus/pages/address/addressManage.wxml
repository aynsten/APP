<view class="content"><view class="row b-b"><text class="tit">省市区</text><block wx:if="{{addressData.provinceName!=undefined}}"><text data-event-opts="{{[['tap',[['openpicker',['$event']]]]]}}" class="input" bindtap="__e">{{''+addressData.provinceName+" "+addressData.cityName+" "+addressData.regionName+''}}</text></block><block wx:else><text data-event-opts="{{[['tap',[['openpicker',['$event']]]]]}}" class="input" bindtap="__e">请选择省市区</text></block></view><view class="row b-b"><text class="tit">名称</text><input class="input" type="text" placeholder="名称" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['$0','storename','$event',[]],['addressData']]]]]}}" value="{{addressData.storename}}" bindinput="__e"/></view><view class="row b-b"><text class="tit">地址</text><input class="input" type="text" placeholder="楼号、门牌" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['$0','addresS1','$event',[]],['addressData']]]]]}}" value="{{addressData.addresS1}}" bindinput="__e"/></view><view class="row default-row"><text class="tit">设为默认</text><switch checked="{{addressData.defaule}}" color="#fa436a" data-event-opts="{{[['change',[['switchChange',['$event']]]]]}}" bindchange="__e"></switch></view><view class="row default-row"><text data-event-opts="{{[['tap',[['removeaddress',['$0'],['addressData']]]]]}}" class="tit" style="color:#FA436A;" bindtap="__e">删除收货地址</text></view><button data-event-opts="{{[['tap',[['confirm',['$event']]]]]}}" class="add-btn" bindtap="__e">提交</button><k-picker vue-id="1" pickershow="{{pickershow}}" datagroup="{{GetRegionAll}}" data-event-opts="{{[['^cancel',[['cancel']]],['^tempcode',[['tempcode']]],['^temptext',[['temptext']]]]}}" bind:cancel="__e" bind:tempcode="__e" bind:temptext="__e" bind:__l="__l"></k-picker></view>