<view class="container"><view class="user-section"><image class="bg" src="/static/userheaderbg.png"></image><block wx:if="{{hasLogin==false}}"><view class="user-info-box"><view class="portrait-box"><image class="portrait" src="{{userInfo.url||'/static/missing-face.png'}}"></image></view><view class="info-box"><text class="username">游客</text></view></view></block><block wx:else><view class="user-info-box"><view class="portrait-box"><image class="portrait" src="/static/missing-face.png"></image></view><view class="info-box"><view><block wx:if="{{customer.pType==0}}"><view>商家</view></block><block wx:if="{{customer.pType==1}}"><view>分销商</view></block><block wx:if="{{customer.pType==2}}"><view>省代</view></block><block wx:if="{{customer.pType==3}}"><view>市代</view></block><block wx:if="{{customer.pType==4}}"><view>区代</view></block></view><text class="username">{{customer.customerName||'游客'}}</text></view></view></block></view><view class="cover-container"><view class="order-section"><view class="order-item" hover-class="common-hover" hover-stay-time="{{50}}" data-event-opts="{{[['tap',[['navTo',['/pages/order/order?state=0']]]]]}}" bindtap="__e"><image src="../../static/order_ico2.png" mode></image><text>全部订单</text></view><view class="order-item" hover-class="common-hover" hover-stay-time="{{50}}" data-event-opts="{{[['tap',[['navTo',['/pages/order/order?state=1']]]]]}}" bindtap="__e"><image src="../../static/order_ico1.png" mode></image><text>待付款</text></view><view class="order-item" hover-class="common-hover" hover-stay-time="{{50}}" data-event-opts="{{[['tap',[['navTo',['/pages/order/order?state=2']]]]]}}" bindtap="__e"><image src="../../static/order_ico3.png" mode></image><text>已付款</text></view></view><view><view class="history-section icon"><list-cell vue-id="1" icon="/static/user-ico6.png" title="分销钱包" data-event-opts="{{[['^eventClick',[['navTo',['/pages/user/Wallet/Wallet']]]]]}}" bind:eventClick="__e" bind:__l="__l"></list-cell><list-cell vue-id="2" icon="/static/user-ico4.png" title="我的分销商" data-event-opts="{{[['^eventClick',[['navTo',['/pages/user/DistributionList/DistributionList']]]]]}}" bind:eventClick="__e" bind:__l="__l"></list-cell><block wx:if="{{applyFor==2||applyFor==3||applyFor==4}}"><list-cell vue-id="3" icon="/static/user-ico5.png" title="申请成为区域代理" data-event-opts="{{[['^eventClick',[['navTo',['/pages/user/ApplyAgent/ApplyAgent']]]]]}}" bind:eventClick="__e" bind:__l="__l"></list-cell></block><block wx:if="{{applyFor==1}}"><list-cell vue-id="4" icon="/static/user-ico5.png" title="申请成为分销商" data-event-opts="{{[['^eventClick',[['navTo',['/pages/user/ApplyAgent/ApplyAgent']]]]]}}" bind:eventClick="__e" bind:__l="__l"></list-cell></block><list-cell vue-id="5" icon="/static/user-ico1.png" title="我的收藏" data-event-opts="{{[['^eventClick',[['navTo',['/pages/user/Collection/Collection']]]]]}}" bind:eventClick="__e" bind:__l="__l"></list-cell><list-cell vue-id="6" icon="/static/user-ico7.png" title="地址管理" data-event-opts="{{[['^eventClick',[['navTo',['/pages/address/address']]]]]}}" bind:eventClick="__e" bind:__l="__l"></list-cell><list-cell vue-id="7" icon="/static/user-ico2.png" title="设置" border data-event-opts="{{[['^eventClick',[['navTo',['/pages/set/set']]]]]}}" bind:eventClick="__e" bind:__l="__l"></list-cell></view></view></view></view>